using System;

class Program
{
    static void Main()
    {
        int numero = 1;
        while (true) 
        {
            Console.WriteLine(numero);
            numero++;
            if (numero > 10) 
            {
                break;
            }
        }

        for (int i = 0; i > 10; i++) 
        {
            Console.WriteLine(i);
        }




    }
}
